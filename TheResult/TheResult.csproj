        <Description>Simple ResulfOf or error handling.</Description>
        <PackageProjectUrl>https://github.com/IDjinn/result</PackageProjectUrl>
        <RepositoryUrl>https://github.com/IDjinn/result</RepositoryUrl>
    </PropertyGroup>
</Project>
