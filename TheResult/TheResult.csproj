        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PublishAot>true</PublishAot>
        <StripSymbols>true</StripSymbols>
        <PackageId>TheResult</PackageId>
        <Version>1.0.0</Version>
        <Authors>IDjinn</Authors>
